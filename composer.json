{
    "name": "disjfa/glynn-admin-symfony4",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony admin project. Basic admin and site setup.",
    "homepage": "https://github.com/disjfa/glynn-admin-symfony4",
    "support": {
        "issues": "https://github.com/disjfa/glynn-admin-symfony4/issues",
        "source": "https://github.com/disjfa/glynn-admin-symfony4"
    },
    "keywords": [
        "symfony project",
        "symfony",
        "site interface",
        "admin interface"
    ],
    "authors": [
        {
            "name": "disjfa",
            "email": "disjfa@disjfa.nl"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "disjfa/glynn-theme-bundle": "dev-master",
        "disjfa/pwa-bundle": "dev-master",
        "disjfa/translation-bundle": "dev-master",
        "league/fractal": "^0.19.2",
        "ornicar/gravatar-bundle": "^1.2",
        "php-translation/symfony-bundle": "^0.9.1",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.4",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "^1.0",
        "symfony/thanks": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "pre-commit": [
            "php-cs-fixer fix",
            "bin/console lint:twig"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
