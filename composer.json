{
    "name": "disjfa/glynn-admin-symfony4",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony admin project. Basic admin and site setup.",
    "homepage": "https://github.com/disjfa/glynn-admin-symfony4",
    "support": {
        "issues": "https://github.com/disjfa/glynn-admin-symfony4/issues",
        "source": "https://github.com/disjfa/glynn-admin-symfony4"
    },
    "keywords": [
        "symfony project",
        "symfony",
        "site interface",
        "admin interface"
    ],
    "authors": [
        {
            "name": "disjfa",
            "email": "disjfa@disjfa.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.2",
        "composer/package-versions-deprecated": "1.11.99.5",
        "disjfa/glynn-theme-bundle": "^0.5.0",
        "disjfa/menu-bundle": "^0.4.0",
        "disjfa/pwa-bundle": "^0.2.0",
        "disjfa/translation-bundle": "^0.5.0",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^2",
        "egulias/email-validator": "^4.0",
        "knplabs/knp-paginator-bundle": "^6.2",
        "league/fractal": "^0.19.2",
        "nyholm/psr7": "^1.8",
        "php-translation/symfony-bundle": "^0.12.1",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/asset": "^6.3",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^6.3",
        "symfony/http-client": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/notifier": "6.3.*",
        "symfony/security-bundle": "^6.3",
        "symfony/sendinblue-mailer": "6.3.*",
        "symfony/templating": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/var-dumper": "^6.3",
        "symfony/yaml": "^6.3",
        "twig/cssinliner-extra": "^3.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/inky-extra": "^3.7",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friendsofphp/php-cs-fixer": "^3",
        "symfony/maker-bundle": "^1.14",
        "symfony/stopwatch": "^6.3",
        "symfony/thanks": "^1.1",
        "symfony/web-profiler-bundle": "^6.3"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/thanks": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "pre-commit": [
            "php-cs-fixer fix",
            "bin/console lint:twig"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    }
}
